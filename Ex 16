#include <iostream>
using namespace std;

int main() {
    int num1, num2;

    cout << "Digite o primeiro número: ";
    cin >> num1;

    cout << "Digite o segundo número: ";
    cin >> num2;

    if (num1 < num2) {
        cout << "Os números em ordem crescente: " << num1 << ", " << num2 << endl;
    } else {
        cout << "Os números em ordem crescente: " << num2 << ", " << num1 << endl;
    }

    return 0;
}
