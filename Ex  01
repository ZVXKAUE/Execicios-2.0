#include <iostream>
using namespace std;

int main() {
    int numero;
       
    cout << "Digite um número: ";
    cin >> numero;
    
    if (numero > 20) {
        cout << "O número é: " << numero << endl;
    } else {
        cout << "O número não é maior que 20." << endl;
    }

    return 0;
}
