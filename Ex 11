#include <iostream>
#include <string>

using namespace std;

int main() {
    string nome;
    char sexo;
    int idade;

    cout << "Digite o nome: ";
    getline(cin, nome);

    cout << "Digite o sexo (M/F): ";
    cin >> sexo;

    cout << "Digite a idade: ";
    cin >> idade;

    if ((sexo == 'f' || sexo == 'F') && idade < 25) {
        cout << nome << ": ACEITA" << endl;
    } else {
        cout << nome << ": NÃƒO ACEITA" << endl;
    }

    return 0;
} 
