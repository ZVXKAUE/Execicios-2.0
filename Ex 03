#include <iostream>
using namespace std;

int main() {
    int num1, num2, soma;
    
    cout << "Digite o primeiro número: ";
    cin >> num1;
    
    cout << "Digite o segundo número: ";
    cin >> num2;
    
    soma = num1 + num2;
    
    if (soma > 20) {
        soma += 8;  
        cout << "Resultado após somar 8: " << soma << endl;
    } else {
        soma -= 5;  
        cout << "Resultado após subtrair 5: " << soma << endl;
    }

    return 0;
}
